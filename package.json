{
  "name": "Kalk",
  "version": "0.0.0",
  "description": "Calculator for desktop",
  "author": "omrirz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omrirz/Kalk"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rm -rf ./dist && rm -rf ./src/config.json",
    "pack": "electron-builder --dir",
    "dist:mac": "npm run clean && electron-builder --dir",
    "dist:win": "npm run clean && electron-builder --win --x64"
  },
  "build": {
    "appId": "BenderRodriguez",
    "mac": {
      "icon": "icon.icns",
      "category": "Calculator"
    },
    "asar":false,
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "win": {
      "icon": "icon.ico",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "runAfterFinish": true
    }

  },
  "devDependencies": {
    "asar": "^3.0.3",
    "electron": "8.0.2",
    "electron-builder": "^22.3.2",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "mathjax-electron": "^3.0.0",
    "mathjs": "^5.10.3",
    "popper.js": "^1.16.1"
  }
}
